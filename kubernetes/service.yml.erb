---
apiVersion: v1
kind: Service
metadata:
  namespace: <%= config['namespace'] %>
  annotations:
    getambassador.io/config: |
      ---
      name: go-links
      kind: Mapping
      host: gcp-go-links.liveramp.net
      prefix: /
      service: go-links.<%= config['namespace'] %>
      timeout_ms: 6000
  labels:
    app: go-links
  name: go-links
spec:
  ports:
  - port: 80
    protocol: TCP
    targetPort: 3000
  selector:
    app: go-links
  type: <%= config['target'] == 'local' ? 'NodePort' : 'ClusterIP' %>

